</span><span class='line'>
</span><span class='line'><span class="s">&lt;p&gt;import com.grygoriy.example.test.imdb.page.MainPage import org.junit.Before import org.junit.Test&lt;/</span><span class="n">p</span><span class="o">&gt;</span>
</span><span class='line'>
</span><span class='line'><span class="o">&lt;</span><span class="n">p</span><span class="o">&gt;</span><span class="kn">import</span> <span class="nn">static</span> <span class="n">com</span><span class="o">.</span><span class="na">grygoriy</span><span class="o">.</span><span class="na">example</span><span class="o">.</span><span class="na">test</span><span class="o">.</span><span class="na">imdb</span><span class="o">.</span><span class="na">util</span><span class="o">.</span><span class="na">UITestHelper</span><span class="o">.</span><span class="na">at</span> <span class="kn">import</span> <span class="nn">static</span> <span class="n">com</span><span class="o">.</span><span class="na">grygoriy</span><span class="o">.</span><span class="na">example</span><span class="o">.</span><span class="na">test</span><span class="o">.</span><span class="na">imdb</span><span class="o">.</span><span class="na">util</span><span class="o">.</span><span class="na">UITestHelper</span><span class="o">.</span><span class="na">go</span> <span class="kn">import</span> <span class="nn">static</span> <span class="n">org</span><span class="o">.</span><span class="na">hamcrest</span><span class="o">.</span><span class="na">collection</span><span class="o">.</span><span class="na">IsCollectionWithSize</span><span class="o">.</span><span class="na">hasSize</span> <span class="kn">import</span> <span class="nn">static</span> <span class="n">org</span><span class="o">.</span><span class="na">junit</span><span class="o">.</span><span class="na">Assert</span><span class="o">.</span><span class="na">assertThat</span><span class="o">&lt;</span><span class="s">/p&gt;</span>
